		"@types/react": "^18.0.32",
		"@vdemedes/prettier-config": "^2.0.1",
		"chalk": "^5.2.0",
		"ts-node": "^10.9.1",
		"tsup": "^8.0.2",
		"typescript": "^5.0.3"
	}
}
